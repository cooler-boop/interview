# 企业级职位搜索系统环境变量配置

# JobSpy API配置
VITE_JOBSPY_API_URL=http://localhost:8000
VITE_JOBSPY_PROXY_POOL=proxy1.com:8080,proxy2.com:8080

# LinkedIn Jobs API配置
# 使用linkedin-jobs-api NPM包，无需API密钥

# JobApis配置
VITE_JOBAPIS_URL=https://api.jobapis.com
VITE_JOBAPIS_KEY=your_jobapis_key_here

# Adzuna API配置
VITE_ADZUNA_APP_ID=your_adzuna_app_id
VITE_ADZUNA_APP_KEY=your_adzuna_app_key

# 代理服务器配置（用于解决CORS问题）
VITE_PROXY_URL=https://cors-anywhere.herokuapp.com/

# 搜索引擎配置
VITE_SEARCH_TIMEOUT=30000
VITE_MAX_RESULTS_PER_ADAPTER=20
VITE_ENABLE_PARALLEL_SEARCH=true
VITE_ENABLE_RESULT_CACHING=true
VITE_CACHE_TIMEOUT=600000

# 速率限制配置
VITE_RATE_LIMIT_REQUESTS_PER_MINUTE=10
VITE_RATE_LIMIT_REQUESTS_PER_HOUR=100

# 系统监控配置
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_TRACKING=true